package quiz17;

public class BugsMusic implements SongList{
	
	private String[] list = new String[100];
	private int count = 0;
	/* SongList인터페이스를 상속받습니다.
	 * 마음대로~
	 * 
	 */
	@Override
	public void insertList(String song) {
		System.out.println("고음질 음악을 재생합니다");
		list[count] = song;
		count += 2;
		
	}
	@Override
	public void playList() {
		System.out.println("순차, 반복 재생합니다.");
		while (true) {
			for (String songlist : list) {
				System.out.println(songlist + " 노래 재생");
			} 
		}
	}
	@Override
	public int playLength() {
		// TODO Auto-generated method stub
		return 0;
	}
	
	
}
