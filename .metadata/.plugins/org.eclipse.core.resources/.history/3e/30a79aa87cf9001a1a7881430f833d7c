package quiz15;

public class Tank extends Unit {
	public Tank() {
		super(0, 0, 100);
	}

	void changeMode() {
		System.out.println("공격모드로 변경");
	}

	@Override
	public void location() {
		System.out.println("Tank의 현재 위치는");
		System.out.println("x : " + getX() + ", y:" + getY());
	}

	@Override
	public void move(int x, int y) {
		
	}
}
