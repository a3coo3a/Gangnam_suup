package quiz15;

public abstract class Unit {
	private int x;
	private int y;
	private int hp;

	Unit(){}
	Unit(int hp) {
		x = 0;
		y = 0;
		this.hp = hp;
	}

	void location() {
		System.out.println("x : " + x + ", y : " + y);
	}

	void move(int x, int y) {
		// x, y값 변경
	}

	void atop() {
		// 현재 위치의 정지
	}
}
