package static_.basic;

class ArrayPrint {

	// 1. 외부에서 객체를 생성하지 못하도록 접근제어자 선언
	// 2. static메서드로 변경해서 사용

	static String printArray(String[] arr) {
		
		String result = "[";
		for(int i = 0; i < arr.length; i++) {
			
			result += arr[i];
			if( i == arr.length - 1) break;
			result += ",";
		}
		result += "]";
		return result;
	}
	
	static String printArray(int[] arr) {
		
		String result = "[";
		for(int i = 0; i < arr.length; i++) {
			
			result += arr[i];
			if( i == arr.length - 1) break;
			result += ",";
		}
		result += "]";
		return result;
	}
	
	
}
