package abs.bad;

public class HeadStore {	// 부모클래스 (본점)
	// 메뉴의 가격을 지점마다 다름
	// 추상메소드 : 부모클래스에서 명시된 메서드는 반드시 자식 클래스에서 각각 오버라이딩 해야 하는 경우
	public void apple() {
		System.out.println("각 지점에서 가격을 제시하세요");
	}
	public void bnanan() {
		System.out.println("각 지점에서 가격을 제시하세요");
	}
	public void melon() {
		System.out.println("각 지점에서 가격을 제시하세요");
	}
	public void orange() {
		System.out.println("각 지점에서 가격을 제시하세요");
	}
}


