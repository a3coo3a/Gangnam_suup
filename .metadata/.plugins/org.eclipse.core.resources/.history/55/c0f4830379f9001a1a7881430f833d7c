package quiz15;

public abstract class Unit {
	private int x;
	private int y;
	private int hp;

	public Unit(int x, int y, int hp) {
		this.x = x;
		this.y = y;
		this.hp = hp;
	}

	void location() {
		System.out.println("x : " + x + ", y : " + y);
	}

	void move(int x, int y) {
		// x, y값 변경
	}

	void atop() {
		// 현재 위치의 정지
	}
}
